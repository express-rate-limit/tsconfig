// tsconfig.json
// The Typescript configuration for express-rate-limit projects.

{
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"moduleResolution": "node"
	}
}
